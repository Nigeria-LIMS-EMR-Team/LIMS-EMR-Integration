                    {
                      "type": "object",
                      "properties": {
                        "IDNumber": {
                          "type": "string"
                        },
                        "IDTypeCode": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "IDNumber",
                        "IDTypeCode"
                      ]
                    }
                  ]
                },
                "firstname": {
                  "type": "string",
				  "minimum": 2,
				"maximum": 32
                },
                "surname": {
                  "type": "string",
				    "minimum": 2,
				"maximum": 32
                },
                "sex": {
                  "type": "string",
				    "minimum": 1,
				"maximum": 1
                },
                "pregnantBreastfeadingStatus": {
                  "type": "string",
				  "minimum": 1,
				"maximum": 5
                },
                "age": {
                  "type": "integer"
                },
                "dob": {
                  "type": "string"
                },
                "facilityID": {
                  "type": "string",
				  "minimum": 11,
				"maximum": 20
                },
                "labID": {
                  "type": "string",
				  "minimum": 11,
				"maximum": 20
                },
                "sampleID": {
                  "type": "string",
				  "minimum": 11,
				"maximum": 20
                },
                "sampleType": {
                  "type": "string",
				  "minimum": 1,
				"maximum": 3
                },
                "indicationVLTest": {
                  "type": "integer"
                },
                "artCommencementDate": {
                  "type": "string"
                },
                "drugRegimen": {
                  "type": "string"
                },
                "sampleOrderedBy": {
                  "type": "string"
                },
                "sampleOrderDate": {
                  "type": "string"
                },
                "sampleCollectedBy": {
                  "type": "string"
                },
                "sampleCollectionDate": {
                  "type": "date"
                },
                "sampleCollectionTime": {
                  "type": "date-time"
                },
                "dateSampleSent": {
                  "type": "date"
                }
              },
              "required": [
                "patientId",
                "firstname",
                "surname",
                "sex",
                "age",
                "dob",
